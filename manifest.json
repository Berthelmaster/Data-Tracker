{
    "manifest_version": 2,
    "name": "Data Tracker",
    "description": "Data Tracker for google chrome - Track who sees your data",
    "version": "1.0.0",
    "icons": {"128": "icon_128.png"},
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permission": ["activeTab", "cookies"]
}